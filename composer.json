{
  "name": "approached/laravel-image-optimizer",
  "description": "Image optimizer for laravel",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "image",
    "optimize",
    "laravel"
  ],
  "homepage": "https://github.com/approached/laravel-image-optimizer",
  "authors": [
    {
      "name": "Alexej Kloos",
      "email": "alexejkloos@gmail.com",
      "homepage": "http://alcodo.com",
      "role": "Developer"
    }
  ],
  "require": {
    "ps/image-optimizer": "dev-laravel-5.6x"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "orchestra/testbench": "3.6.x@dev"
  },
  "autoload": {
    "psr-4": {
      "Approached\\LaravelImageOptimizer\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Approached\\LaravelImageOptimizer\\ServiceProvider"
      ]
    }
  }
}
